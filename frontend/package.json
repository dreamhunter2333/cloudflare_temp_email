    "axios": "^1.9.0",
    "jszip": "^3.10.1",
    "mail-parser-wasm": "^0.2.1",
    "naive-ui": "^2.41.0",
    "postal-mime": "^2.4.3",
    "vooks": "^0.2.12",
    "vue": "^3.5.13",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^11.1.3",
